{
  "name": "@across-protocol/sdk-v2",
  "author": "UMA Team",
  "version": "0.0.3",
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "test:e2e": "tsdx test --config jest-e2e-config.json",
    "lint": "tsdx lint --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "module": "dist/sdk-v2.esm.js",
  "size-limit": [
    {
      "path": "dist/sdk-v2.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/sdk-v2.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@eth-optimism/contracts": "^0.5.18",
    "@size-limit/preset-small-lib": "^7.0.8",
    "dotenv": "^16.0.0",
    "@types/dotenv": "^8.2.0",
    "hardhat": "^2.9.1",
    "husky": "^7.0.4",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typechain": "^7.0.1",
    "typescript": "^4.6.2",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@typechain/hardhat": "^5.0.0"
  },
  "dependencies": {
    "@across-protocol/contracts-v2": "^0.0.29",
    "@uma/sdk": "^0.22.1",
    "decimal.js": "^10.3.1",
    "ethers": "^5.6.1",
    "hardhat-deploy": "^0.11.2",
    "hardhat-gas-reporter": "^1.0.8"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com/",
    "access": "public"
  }
}
